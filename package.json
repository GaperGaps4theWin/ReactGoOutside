{
  "name": "gooutside-webpage",
  "version": "1.0.0",
  "description": "GoOutside Designs webpage useing react with webpack babel and bootstrap.",
  "main": "index.js",
  "scripts": {
    "start": " npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline",
    "build:prod": "webpack -p && cp src/index.html dist/index.html "
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Jack Thibodeau",
  "license": "MIT",
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
